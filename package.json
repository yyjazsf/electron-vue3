{
  "name": "electron-vue-scaffold",
  "version": "0.1.0",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "",
    "serve": "vue-cli-service serve renderer/renderer.ts",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "electron:watch": "cd background && tsc -w",
    "electron:dev": "cd background && tsc && electron dist/app.js",
    "electron:pack-mac": "electron-builder --mac --config electron-builder.yml",
    "electron:pack-win32": "electron-builder --win --ia32 --config electron-builder.yml",
    "electron:serve": "npm-run-all --parallel serve electron:watch electron:dev",
    "electron:build": "node scripts/build"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-log": "^4.3.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "electron": "8.2.0",
    "electron-builder": "^22.9.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "shelljs": "^0.8.4",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-0"
  }
}
